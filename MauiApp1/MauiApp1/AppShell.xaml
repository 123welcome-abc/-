<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    Shell.FlyoutBehavior="Disabled">

    <Tab Title="Stack">
        <ShellContent Title="Stack" ContentTemplate="{DataTemplate local:StackPage}" />
    </Tab>
    <Tab Title="CollectionView">
        <ShellContent Title="CollectionView" ContentTemplate="{DataTemplate local:CollectionViewPage}" />
    </Tab>
    <Tab Title="Binding">
        <ShellContent Title="Binding" ContentTemplate="{DataTemplate local:BindingPage}" />
    </Tab>
</Shell>
